{
  "$schema": "./node_modules/oxfmt/configuration_schema.json",
  "printWidth": 130,
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "rangeStart": 0,
  "rangeEnd": null,
  "requirePragma": false,
  "insertPragma": false,
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "css",
  "endOfLine": "lf",
  "organizeImportsSkipDestructiveCodeActions": true,
  "sortPackageJson": {},
  "sortImports": {},
  "ignorePatterns": ["dist", "**/.DS_Store", "node_modules", "packages/shared/theme/colorUtils.js"]
}
